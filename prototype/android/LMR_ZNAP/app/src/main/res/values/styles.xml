<resources>

    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar"/>
    <!-- Customize your theme here. -->
    <style name="AppDefault" parent="AppTheme">
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorControlNormal">@color/colorWhite</item>
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="PrimaryButton" parent="Theme.AppCompat">
        <item name="colorButtonNormal">@color/colorAccent</item>
        <item name="colorControlHighlight">@color/colorAccent</item>
    </style>

    <style name="test" parent="Theme.AppCompat">
        <item name="colorButtonNormal">@color/test</item>
    </style>
    <style name="mainActButton" parent="Theme.AppCompat">
        <item name="colorButtonNormal">@color/colorNonActive</item>
        <item name="colorControlHighlight">@color/colorAccent</item>
    </style>
    <style name="PrimaryFlatButton" parent="AppTheme">
        <item name="android:buttonStyle">@style/Widget.AppCompat.Button.Borderless.Colored</item>
        <item name="colorControlHighlight">@color/colorAccent</item>
        <item name="colorAccent">@color/colorPrimary</item>
    </style>



</resources>
